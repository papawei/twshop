<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tw.mapper.UserMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.tw.model.User">
		<id column="id" property="id" />
		<result column="mobile" property="mobile" />
		<result column="wechat" property="wechat" />
		<result column="state" property="state" />
		<result column="add_time" property="addTime" />
		<result column="desc" property="desc" />
		<result column="address" property="address" />
		<result column="guid" property="guid" />
		<result column="path" property="path" />
		<result column="layer" property="layer" />
		<result column="parent_id" property="parentId" />
		<result column="type" property="type" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, mobile, wechat, state, add_time AS addTime, desc, address, guid, path, layer, parent_id AS parentId, type, update_time AS updateTime
    </sql>

</mapper>
