<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tw.mapper.ShoppingCartMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.tw.model.ShoppingCart">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="sku_id" property="skuId" />
		<result column="add_time" property="addTime" />
		<result column="num" property="num" />
		<result column="update_time" property="updateTime" />
		<result column="product_id" property="productId" />
		<result column="shop_id" property="shopId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, sku_id AS skuId, add_time AS addTime, num, update_time AS updateTime, product_id AS productId, shop_id AS shopId
    </sql>

</mapper>
